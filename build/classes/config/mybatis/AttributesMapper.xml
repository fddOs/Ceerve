<?xml version= "1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.gaokao.refen.mapper.AttributesMapper">
	<resultMap type="attribute" id="attribute">
		<id column="ATTRNO" property="attrNo" />
		<result column="ATTRNAME" property="attrName" />
	</resultMap>
	<select id="getAttributes" resultMap="attribute">
		SELECT
		ATTRNO,
		ATTRNAME
		FROM
		ATTRIBUTE
	</select>
</mapper>
	
	
	
	